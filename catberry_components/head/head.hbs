<meta charset="UTF-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<title>{{page.title}} ::: {{page.subtitle}}</title>
<meta name="description" content="{{l10n "DESCRIPTION"}}">
<meta name="keywords" content="{{l10n "KEYWORDS"}}">

<link rel="canonical" href="{{location}}" />
<!--<meta name="DC.date.issued" content="2007-04-28">-->
<meta name="application-name" content="{{page.title}}" />

<!-- Begin Twitter summary card -->
<meta name="twitter:card" content="summary" />
<meta name="twitter:site" content="@jbokiev" />
<meta name="twitter:title" content="{{page.title}}" />
<meta name="twitter:description" content="{{l10n "DESCRIPTION"}}" />
<meta name="twitter:image" content="{{socialLogo}}" />
<meta name="twitter:url" content="{{location}}" />
<!-- End Twitter summary card-->

<!-- Begin Open Graph markup -->
<meta property="og:title" content="{{page.title}}" />
<meta property="og:description" content="{{l10n "DESCRIPTION"}}" />
<!-- article, website -->
<meta property="og:type" content="website" />
<meta property="og:url" content="{{location}}" />
<!-- Example: http://newrelic.com/assets/newrelic/logo_NR-fb.png -->
<meta property="og:image" content="{{socialLogo}}" />

